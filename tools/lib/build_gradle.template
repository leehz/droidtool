buildscript {
    repositories {
        mavenCentral()
    }
    dependencies {
        classpath 'com.android.tools.build:gradle:ARTIFACT_VERSION'
    }
}
apply plugin: 'PLUGIN'

android {
    compileSdkVersion 'TARGET'
    buildToolsVersion 'BUILD_TOOL_REV'

    buildTypes {
        release {
            minifyEnabled false
            proguardFile getDefaultProguardFile('proguard-android.txt')
        }
    }
}
